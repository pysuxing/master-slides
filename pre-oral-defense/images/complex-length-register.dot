digraph g {
        node [shape=ellipse];
        result;
        sqrt;
        t1[shape=point];
        plus[label="+"]
        t2[shape=point];
        t3[shape=point];
        mul1[label="*"];
        x1[label="x"];
        x2[label="x"];
        mul2[label="*"];
        y1[label="y"];
        y2[label="y"];

        result -> sqrt;
        result -> t1[color=red];
        t1 -> plus;
        t1 -> t2[color=red];
        t1 -> t3[color=blue];
        t2 -> mul1;
        t2 -> x1[color=red];
        t2 -> x2;
        t3 -> mul2;
        t3 -> y1[color=blue];
        t3 -> y2;
}