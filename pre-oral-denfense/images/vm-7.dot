digraph g {
        node [shape=ellipse];
        result;
        sqrt;
        t1[shape=diamond, label=""];
        plus[label="+"]
        t2[shape=diamond, label=""];
        t3[shape=diamond, label=""];
        mul1[label="*"];
        x1[label="x", style=filled, fillcolor=gray];
        x2[label="x"];
        mul2[label="*"];
        y1[label="y"];
        y2[label="y"];

        result -> sqrt;
        result -> t1;
        t1 -> plus;
        t1 -> t2;
        t1 -> t3;
        t2 -> mul1;
        t2 -> x1;
        t2 -> x2;
        t3 -> mul2;
        t3 -> y1;
        t3 -> y2;
}